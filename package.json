{
  "name": "ifc-lbd",
  "version": "0.0.20",
  "description": "",
  "main": "lib/cjs/index.js",
  "module": "lib/esm/index.js",
  "types": "lib/types/index.d.ts",
  "repository": {
    "url": "https://github.com/LBD-Hackers/IFC-LBD",
    "type": "git"
  },
  "scripts": {
    "build": "npm-run-all -l clean -p build:esm build:cjs build:bundles build:cli-tool declarations",
    "clean": "rimraf lib",
    "declarations": "tsc -p tsconfig.json",
    "build:bundles": "cross-env BABEL_ENV=esmBundled rollup -c",
    "build:cli-tool": "cross-env BABEL_ENV=esmBundled rollup -c rollup.cli-config.js",
    "build:esm": "cross-env BABEL_ENV=esmUnbundled babel src --extensions '.ts' --out-dir 'lib/esm' --source-maps",
    "build:cjs": "cross-env BABEL_ENV=cjs babel src --extensions '.ts' --out-dir 'lib/cjs' --source-maps",
    "copy-assets": "cp node_modules/web-ifc/web-ifc.wasm lib && cp node_modules/web-ifc/web-ifc-mt.wasm lib",
    "test": "jest ./tests"
  },
  "keywords": [],
  "author": "Mads Holten Rasmussen<mhra@niras.dk>",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/plugin-transform-modules-commonjs": "^7.16.5",
    "@babel/plugin-transform-runtime": "^7.16.5",
    "@babel/preset-env": "^7.16.5",
    "@babel/preset-typescript": "^7.16.5",
    "@babel/runtime": "^7.16.5",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.1.1",
    "@types/jest": "^27.0.3",
    "@types/jsonld": "^1.5.6",
    "@types/meow": "^6.0.0",
    "@types/yargs": "^17.0.7",
    "babel-jest": "^27.4.5",
    "babel-polyfill": "^6.26.0",
    "jest": "^27.4.5",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "rollup": "^2.61.1",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-dts": "^4.1.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-preserve-shebang": "^1.0.1",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.31.1",
    "ts-jest": "^27.1.2",
    "ts-node": "^10.4.0",
    "tslib": "^2.3.1",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "comunica-geosparql": "^0.0.1",
    "jsonld": "^5.2.0",
    "point-in-polygon": "^1.1.0",
    "web-ifc": "^0.0.30",
    "yargs": "^17.3.0"
  },
  "peerDependencies": {
    "@comunica/actor-init-sparql-rdfjs": "^1.22.3",
    "n3": "^1.12.2"
  },
  "browser": {
    "crypto": false,
    "path": false,
    "fs": false
  },
  "files": [
    "lib/*"
  ],
  "bin": {
    "ifc-lbd": "./lib/cli-tool/index.js"
  }
}
