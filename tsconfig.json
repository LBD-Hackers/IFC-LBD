{
  "compilerOptions": {
    "target": "ES2015",
    "module": "esnext",
    "strict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "./out-tsc",
    "moduleResolution": "node",
    "skipLibCheck": true,
    "declaration": true,
    "resolveJsonModule": true
  },
  "include": [
    "./src/**/*.ts"
  ]
}